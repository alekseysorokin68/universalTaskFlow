<?xml version='1.0' encoding='UTF-8'?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page"
          version="2.1"
          xmlns:f="http://java.sun.com/jsf/core"
          xmlns:h="http://java.sun.com/jsf/html"
          xmlns:trh="http://myfaces.apache.org/trinidad/html"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:af="http://xmlns.oracle.com/adf/faces/rich">
  <jsp:directive.page contentType="text/html;charset=UTF-8"/>
  <af:componentDef var="attrs" componentVar="component">
    <af:xmlContent>
      <component xmlns="http://xmlns.oracle.com/adf/faces/rich/component">
        <display-name>formFieldsOnSqlDC</display-name>
        <component-extension>
          <component-tag-namespace>universal_taskflow.declarative_component</component-tag-namespace>
          <component-taglib-uri>/componentLibTf</component-taglib-uri>
        </component-extension>
      </component>
    </af:xmlContent>

    <af:panelSplitter orientation="vertical"
                          styleClass="AFStretchWidth"
                          splitterPosition="128"
                          rendered="#{not empty backingBeanScope.editDefaultsBean.paramsForOnSql.sqlAttributes}"
                          id="dc_ps1">
              <c:set var="isAdfTable"  value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.mainRecord.taskFlowType.name == 'TABLE_ADF'}"  scope="page" />
              <c:set var="isHtmlTable" value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.mainRecord.taskFlowType.name == 'TABLE_HTML'}" scope="page" />
              <c:set var="isForm"      value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.mainRecord.taskFlowType.name == 'FORM'}"       scope="page" />
              <c:set var="isTree"      value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.mainRecord.taskFlowType.name == 'TREE'}"       scope="page" />
              <c:set var="isDiagram"   value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.mainRecord.taskFlowType.isDiagram}"            scope="page" />

              <f:facet name="first">
                <af:panelStretchLayout id="dc_psl2" topHeight="#{'auto'}">
                  <f:facet name="top">
                    <af:panelGroupLayout layout="vertical"
                                         id="dc_pgl8"
                                         styleClass="AFStrechLayout"
                                         halign="right"

                                         >
                      <af:panelGroupLayout  id="dc_pgl9" layout="horizontal">
                        <af:toolbar id="t6" >
                          <af:menuBar id="mb1">
                            <af:menu text="Ширины" id="m2">
                              <af:commandMenuItem text="Равномерно разделить колонки по ширине"
                                                  shortDesc="В %"
                                                  partialSubmit="false"
                                                  action="#{backingBeanScope.editDefaultsBean.paramsForOnSql.setFieldsWidthEvenlyAction}"
                                                  id="cl1"/>
                              <af:commandMenuItem text="Почистить поле 'Ширина' у всех колонок"
                                                  partialSubmit="false"
                                                  action="#{backingBeanScope.editDefaultsBean.paramsForOnSql.clearFieldsWidthInTable}"
                                                  id="cl2"/>
                            </af:menu>
                            <af:menu text="Запросы" id="m3">
                              <af:commandMenuItem text="Запрос на вставку"
                                                  partialSubmit="false"
                                                  action="#{backingBeanScope.editDefaultsBean.paramsForOnSql.getSqlInsertAction}"
                                                  id="cmi1"/>
                              <af:commandMenuItem text="Запрос на корректировку"
                                                  partialSubmit="false"
                                                  action="#{backingBeanScope.editDefaultsBean.paramsForOnSql.getSqlUpdateAction}"
                                                  id="cmi2"/>
                              <af:commandMenuItem text="Запрос на удаление"
                                                  partialSubmit="false"
                                                  action="#{backingBeanScope.editDefaultsBean.paramsForOnSql.getSqlDeleteAction}"
                                                  id="cmi3"/>
                            </af:menu>

                            <af:menu text="Выбор и порядок полей" id="dc_m1">
                              <af:commandMenuItem text="В таблице"
                                                  partialSubmit="false"
                                                  id="dc_cmi1">
                                  <af:showPopupBehavior
                                              popupId="popup_order_table"
                                              triggerType="action"
                                  />
                              </af:commandMenuItem>

                              <af:commandMenuItem text="В форме"
                                                  partialSubmit="false"
                                                  id="dc_cmi2">
                                  <af:showPopupBehavior
                                              popupId="popup_order_form"
                                              triggerType="action"
                                  />
                              </af:commandMenuItem>


                            </af:menu>
                          </af:menuBar>
                        </af:toolbar>
                        <af:popup id="popup_order_table" contentDelivery="lazyUncached">
                          <af:dialog id="dc_d3" 
                                     title="Выбор и порядок полей в таблице" 
                                     stretchChildren="first"
                                     type="none"
                                     closeIconVisible="false"
                                     resize="off"
                                     contentWidth="600"
                                     >
                            <af:declarativeComponent viewId="/universal_taskflow/pages/edit_defaults/type_parameters/on_sql/selectOrderShuttleFieldsDC.jsff"
                                                     id="dc_dc6">
                                 <f:attribute name="parentComponent" value="table" />                    
                            </af:declarativeComponent>
                          </af:dialog>
                        </af:popup>

                        <af:popup id="popup_order_form">
                          <af:dialog id="dc_d4" 
                                     title="Выбор и порядок полей в форме"
                                     stretchChildren="first"
                                     type="none"
                                     closeIconVisible="false"
                                     resize="off"
                                     contentWidth="600"
                          >
                            <af:declarativeComponent viewId="/universal_taskflow/pages/edit_defaults/type_parameters/on_sql/selectOrderShuttleFieldsDC.jsff"
                                                     id="dc_dc7">
                                 <f:attribute name="parentComponent" value="form" />                    
                            </af:declarativeComponent>
                          </af:dialog>
                        </af:popup>
                      </af:panelGroupLayout>
                    </af:panelGroupLayout>
                  </f:facet>
                  <f:facet name="center">
                  <!--
                  partialTriggers="::dc1:dc_it5 ::dc1"
                  -->
                    <af:table id="tFields"
                              value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.sqlAttributesList}"
                              rowSelection="single"
                              var="row"
                              varStatus="vs"
                              summary="Таблица полей"
                              styleClass="AFStretchWidth"
                              selectionListener="#{backingBeanScope.editDefaultsBean.paramsForOnSql.sqlAttributeSelectionListener}"
                              columnStretching="last"
                              columnResizing="disabled"
                              emptyText="Нет полей"
                              disableColumnReordering="true"
                              contentDelivery="whenAvailable"
                              partialTriggers="::dc1:dc_it5"
                              binding="#{backingBeanScope.editDefaultsBean.paramsForOnSql.tableSqlAttributes}">
                      <af:column rowHeader="true" filterable="false"
                                 sortable="false" headerNoWrap="true"
                                 headerText="№" align="left" width="26px"
                                 frozen="true" id="dc_c1">
                        <af:outputText id="dc_ot1" value="#{vs.index + 1}"/>
                      </af:column>
                      <af:column rowHeader="#{false}" filterable="false"
                                 sortable="false" headerNoWrap="false"
                                 headerText="Имя" width="236px" align="left"
                                 id="dc_c2">
                        <af:outputText id="dc_ot3" value="#{row.name}"/>
                      </af:column>
                      <af:column rowHeader="#{false}" filterable="false"
                                 sortable="false" headerNoWrap="false"
                                 headerText="Тип" width="96px" align="left"
                                 id="dc_c3">
                        <af:outputText id="dc_ot4" value="#{row.sqlTypeName}"/>
                      </af:column>

                      <af:column filterable="false"
                                 sortable="false"
                                 headerText="Длина" width="40px" align="right"
                                 id="dc_c5">
                        <af:outputText id="dc_ot2" value="#{row.length}"/>
                      </af:column>

                      <af:column filterable="false"
                                 sortable="false"
                                 headerText="Обнуляемо"
                                 width="64px"
                                 align="center"
                                 id="dc_c6">
                        <af:switcher facetName="#{row.nullable ? 'SELECTED' : 'NO_SELECTED'}"
                                     id="dc_s5">
                          <f:facet name="SELECTED">
                              <af:selectBooleanCheckbox
                                label="#{null}"
                                simple="false"
                                readOnly="true"
                                id="dc_ot5"
                                value="true"
                                />
                          </f:facet>
                          <f:facet name="NO_SELECTED">
                            <af:outputText id="dc_ot6"/>
                          </f:facet>
                        </af:switcher>
                      </af:column>

                      <af:column filterable="false"
                                 sortable="false"
                                 headerNoWrap="false"
                                 headerText="Заголовок поля"
                                 align="left"
                                 id="dc_c4">
                              <af:inputText label="#{null}"
                                            simple="true"
                                            value="#{row.caption}"
                                            id="dc_it4"
                                            autoSubmit="true"
                                            immediate="true"
                                            shortDesc="Здесь можно вводить непосредственное значение поля. Если вы хотите задать EL или HTML выражение, то сделайте  это в разделе 'Основные данные'. В этом случае будет действовать, то что задано там, а не здесь."
                                            styleClass="AFStretchWidth"/>
                      </af:column>
                    </af:table>
                  </f:facet>
                </af:panelStretchLayout>
              </f:facet>
              <f:facet name="second">
                <af:panelStretchLayout id="dc_psl13"
                                       styleClass="AFStretchWidth"
                                       bottomHeight="0"
                                       partialTriggers="tFields"
                                       >
                  <f:facet name="center">
                    <af:panelGroupLayout id="pgl13"
                                         styleClass="AFStretchWidth"
                                         partialTriggers="tFields"
                                         rendered="#{backingBeanScope.editDefaultsBean.paramsForOnSql.currentSqlAttribute != null}"
                                         layout="scroll">
                      <af:panelBox text="Поле #{backingBeanScope.editDefaultsBean.paramsForOnSql.currentSqlAttribute.name}"
                                   id="dc_pb6">
                        <af:panelBox text="Основные данные" id="dc_pb2">
                          <af:panelFormLayout id="dc_pfl2"
                                              rows="#{isAdfTable ? 8 :
                                                     (isHtmlTable ? 6 :
                                                     (isForm ? 5 :
                                                     (isTree ? 6 :
                                                     (isDiagram ? 4 :
                                                     6))))}"
                          >
                            <af:panelLabelAndMessage id="dc_plam1"
                                                     label="Заголовок поля"
                                                     labelStyle="text-align:start"
                                                     for="dc1:dc_it5"
                            >
                            <!--
                            partialTriggers="::tFields:dc1a:dc_it4 ::tFields:dc1a"
                            -->
                              <af:declarativeComponent
                                  viewId="/universal_taskflow/pages/edit_defaults/valueEditorWithElHtmlCompDef.jsff"
                                  partialTriggers="::tFields:dc_it4"
                                  id="dc1">
                                  <f:attribute name="propertyClassName" value="universal_taskflow.common.data.SqlAttribute" />
                                  <f:attribute name="propertyName" value="caption" />
                                  <f:attribute name="objectId" value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.currentSqlAttribute.name}" />
                                  <f:attribute name="htmlDisabled" value="#{false}" />
                                  <f:attribute name="elDisabled" value="#{false}" />
                                  <f:attribute name="clearTargetProperty" value="#{true}" />
                                  <f:attribute name="object" value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.currentSqlAttribute}" />
                                  <f:facet name="content">
                                    <af:inputText id="dc_it5"
                                                  label="#{null}"
                                                  value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.currentSqlAttribute.caption}"
                                                  autoSubmit="true"
                                                  immediate="true"
                                                  simple="true">
                                    </af:inputText>
                                  </f:facet>
                              </af:declarativeComponent>
                            </af:panelLabelAndMessage>
                            <af:panelLabelAndMessage id="dc_plam2"
                                                     label="Всплывающий текст"
                                                     labelStyle="text-align:start"
                                                     for="dc_dc1:dc_it6"
                            >
                              <af:declarativeComponent
                                viewId="/universal_taskflow/pages/edit_defaults/valueEditorWithElHtmlCompDef.jsff"
                                id="dc_dc1">
                                  <f:attribute name="propertyClassName" value="universal_taskflow.common.data.SqlAttribute" />
                                  <f:attribute name="propertyName" value="schortDesc" />
                                  <f:attribute name="objectId" value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.currentSqlAttribute.name}" />
                                  <f:attribute name="htmlDisabled" value="#{true}" />
                                  <f:attribute name="elDisabled" value="#{false}" />
                                  <f:facet name="content">
                                      <af:inputText id="dc_it6"
                                                    label="#{null}"
                                                    value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.currentSqlAttribute.schortDesc}"
                                                    simple="true"/>
                                  </f:facet>
                               </af:declarativeComponent>
                            </af:panelLabelAndMessage>


                            <af:panelLabelAndMessage id="dc_plam3"
                                                     label="Видимость поля в таблице"
                                                     labelStyle="text-align:start"
                                                     for="dc_dc2:dc_sbc4"
                                                     rendered="#{isAdfTable || isHtmlTable || isTree}"
                                                     >
                                <af:declarativeComponent
                                    viewId="/universal_taskflow/pages/edit_defaults/valueEditorWithElHtmlCompDef.jsff"
                                    id="dc_dc2"
                                    >
                                  <f:attribute name="propertyClassName" value="universal_taskflow.common.data.SqlAttribute" />
                                  <f:attribute name="propertyName" value="visibleInTable" />
                                  <f:attribute name="objectId" value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.currentSqlAttribute.name}" />
                                  <f:attribute name="htmlDisabled" value="#{true}" />
                                  <f:attribute name="elDisabled" value="#{false}" />
                                  <f:facet name="content">
                                    <af:selectBooleanCheckbox id="dc_sbc4"
                                        label="#{null}"
                                        simple="true"
                                        value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.currentSqlAttribute.visibleInTable}"
                                    />
                                  </f:facet>
                                </af:declarativeComponent>


                            </af:panelLabelAndMessage>

                            <af:panelLabelAndMessage id="dc_plam23"
                                                     label="Видимость поля в форме"
                                                     labelStyle="text-align:start"
                                                     for="dc_dc3:dc_sbc5"
                                                     rendered="#{isAdfTable || isHtmlTable || isForm || isTree}"
                                                     >
                                <af:declarativeComponent
                                    viewId="/universal_taskflow/pages/edit_defaults/valueEditorWithElHtmlCompDef.jsff"
                                    id="dc_dc3">
                                  <f:attribute name="propertyClassName" value="universal_taskflow.common.data.SqlAttribute" />
                                  <f:attribute name="propertyName" value="visibleInForm" />
                                  <f:attribute name="objectId" value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.currentSqlAttribute.name}" />
                                  <f:attribute name="htmlDisabled" value="#{true}" />
                                  <f:attribute name="elDisabled" value="#{false}" />
                                  <f:facet name="content">
                                      <af:selectBooleanCheckbox id="dc_sbc5"
                                          label="#{null}"
                                          simple="true"
                                          value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.currentSqlAttribute.visibleInForm}"
                                      />
                                   </f:facet>
                               </af:declarativeComponent>

                            </af:panelLabelAndMessage>



                            <af:panelLabelAndMessage id="dc_plam5"
                                                     label="Ширина поля в таблице"
                                                     labelStyle="text-align:start"
                                                     for="dc_dc4:dc_sbc6"
                                                     rendered="#{isAdfTable || isHtmlTable || isTree}"

                            >

                                <af:declarativeComponent
                                    viewId="/universal_taskflow/pages/edit_defaults/valueEditorWithElHtmlCompDef.jsff"
                                    id="dc_dc4"
                                    >
                                      <f:attribute name="propertyClassName" value="universal_taskflow.common.data.SqlAttribute" />
                                      <f:attribute name="propertyName" value="widthInTable" />
                                      <f:attribute name="objectId" value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.currentSqlAttribute.name}" />
                                      <f:attribute name="htmlDisabled" value="#{true}" />
                                      <f:attribute name="elDisabled" value="#{false}" />
                                      <f:facet name="content">
                                          <af:inputText id="dc_sbc6"
                                                        label="#{null}"
                                                        simple="true"
                                                        value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.currentSqlAttribute.widthInTable}"
                                                        contentStyle="width:62px"
                                                        />
                                      </f:facet>
                                </af:declarativeComponent>

                            </af:panelLabelAndMessage>

                            <af:panelLabelAndMessage id="dc_plam5d"
                                                     label="Ширина поля в форме"
                                                     labelStyle="text-align:start"
                                                     for="dc_dc5:dc_sbc7"
                                                     rendered="#{isAdfTable || isHtmlTable || isTree || isForm}"

                            >


                                <af:declarativeComponent
                                    viewId="/universal_taskflow/pages/edit_defaults/valueEditorWithElHtmlCompDef.jsff"
                                    id="dc_dc5">
                                      <f:attribute name="propertyClassName" value="universal_taskflow.common.data.SqlAttribute" />
                                      <f:attribute name="propertyName" value="widthInForm" />
                                      <f:attribute name="objectId" value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.currentSqlAttribute.name}" />
                                      <f:attribute name="htmlDisabled" value="#{true}" />
                                      <f:attribute name="elDisabled" value="#{false}" />
                                      <f:facet name="content">
                                          <af:inputText id="dc_sbc7"
                                                        label="#{null}"
                                                        simple="true"
                                                        value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.currentSqlAttribute.widthInForm}"
                                                        contentStyle="width:62px"/>
                                      </f:facet>
                                </af:declarativeComponent>
                            </af:panelLabelAndMessage>




                            <af:panelLabelAndMessage id="dc_plam7"
                                                     label="Выравнивание заголовка в таблице:"
                                                     labelStyle="text-align:start;white-space: nowrap"
                                                     for="dc_dc12:dc_soc1b"
                                                     rendered="#{isAdfTable || isHtmlTable}"
                            >
                              <af:declarativeComponent
                                      viewId="/universal_taskflow/pages/edit_defaults/valueEditorWithElHtmlCompDef.jsff"
                                      id="dc_dc12">
                                  <f:attribute name="propertyClassName" value="universal_taskflow.common.data.SqlAttribute" />
                                  <f:attribute name="propertyName" value="headerAlign" />
                                  <f:attribute name="objectId" value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.currentSqlAttribute.name}" />
                                  <f:attribute name="htmlDisabled" value="#{true}" />
                                  <f:attribute name="elDisabled" value="#{false}" />
                                  <f:facet name="content">
                                        <af:selectOneChoice
                                            id="dc_soc1b"
                                            label="Заголовка"
                                            simple="true"
                                            value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.currentSqlAttribute.headerAlign}"
                                        >
                                          <f:selectItems value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.horizontalAlignSelectItems}"
                                                         id="dc_si2"/>
                                        </af:selectOneChoice>
                                   </f:facet>
                                </af:declarativeComponent>

                            </af:panelLabelAndMessage>

                            <af:panelLabelAndMessage id="dc_plam15"
                                                     label="Выравнивание данных в таблице:"
                                                     labelStyle="text-align:start;white-space: nowrap"
                                                     for="dc_dc13:dc_soc6"
                                                     rendered="#{isAdfTable || isHtmlTable}"
                            >
                              <af:declarativeComponent
                                      viewId="/universal_taskflow/pages/edit_defaults/valueEditorWithElHtmlCompDef.jsff"
                                      id="dc_dc13">
                                  <f:attribute name="propertyClassName" value="universal_taskflow.common.data.SqlAttribute" />
                                  <f:attribute name="propertyName" value="dataAlign" />
                                  <f:attribute name="objectId" value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.currentSqlAttribute.name}" />
                                  <f:attribute name="htmlDisabled" value="#{true}" />
                                  <f:attribute name="elDisabled" value="#{false}" />
                                  <f:facet name="content">
                                    <af:selectOneChoice
                                        id="dc_soc6"
                                        label="Данных"
                                        value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.currentSqlAttribute.dataAlign}"
                                        simple="true"
                                    >
                                      <f:selectItems value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.horizontalAlignSelectItems}"
                                                     id="dc_si1b"/>
                                    </af:selectOneChoice>
                                  </f:facet>
                                </af:declarativeComponent>
                            </af:panelLabelAndMessage>

                            <af:panelLabelAndMessage  label="Первичный ключ"
                                                      labelStyle="text-align:start;white-space: nowrap"
                                                      for="sbc3"
                                                      id="plam6">
                                <af:selectBooleanCheckbox label="#{null}"
                                                          id="sbc3"
                                                          simple="true"
                                                          value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.currentSqlAttribute.primaryKey}">
                                </af:selectBooleanCheckbox>
                              </af:panelLabelAndMessage>

                            <af:panelLabelAndMessage id="dc_plam8"
                                                     label="Перенос заголовка в таблице"
                                                     labelStyle="text-align:start"
                                                     for="dc_dc14:dc_soc3"
                                                     rendered="#{isAdfTable || isHtmlTable}"
                            >
                              <af:declarativeComponent
                                      viewId="/universal_taskflow/pages/edit_defaults/valueEditorWithElHtmlCompDef.jsff"
                                      id="dc_dc14">
                                  <f:attribute name="propertyClassName" value="universal_taskflow.common.data.SqlAttribute" />
                                  <f:attribute name="propertyName" value="wrapEnabled" />
                                  <f:attribute name="objectId" value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.currentSqlAttribute.name}" />
                                  <f:attribute name="htmlDisabled" value="#{true}" />
                                  <f:attribute name="elDisabled" value="#{false}" />
                                  <f:facet name="content">
                                      <af:selectBooleanCheckbox
                                          id="dc_soc3"
                                          label="Заголовка"
                                          simple="true"
                                          value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.currentSqlAttribute.wrapEnabled}"
                                      />
                                  </f:facet>
                              </af:declarativeComponent>
                            </af:panelLabelAndMessage>

                            <af:panelLabelAndMessage id="dc_plam16"
                                                     label="Перенос данных в таблице"
                                                     labelStyle="text-align:start"
                                                     for="dc_dc15:dc_soc4b"
                                                     rendered="#{isAdfTable || isHtmlTable}"
                            >
                              <af:declarativeComponent
                                      viewId="/universal_taskflow/pages/edit_defaults/valueEditorWithElHtmlCompDef.jsff"
                                      id="dc_dc15">
                                  <f:attribute name="propertyClassName" value="universal_taskflow.common.data.SqlAttribute" />
                                  <f:attribute name="propertyName" value="wrapEnabledData" />
                                  <f:attribute name="objectId" value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.currentSqlAttribute.name}" />
                                  <f:attribute name="htmlDisabled" value="#{true}" />
                                  <f:attribute name="elDisabled" value="#{false}" />
                                  <f:facet name="content">
                                      <af:selectBooleanCheckbox id="dc_soc4b"
                                          label="Данных"
                                          inlineStyle="width:105px"
                                          simple="true"
                                          value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.currentSqlAttribute.wrapEnabledData}"
                                      />
                                  </f:facet>
                                </af:declarativeComponent>
                            </af:panelLabelAndMessage>

                            <af:panelLabelAndMessage id="dc_plam9"
                                                     label="Наличие фильтра в таблице"
                                                     labelStyle="text-align:start"
                                                     for="dc_dc16:dc_sbc8"
                                                     rendered="#{isAdfTable}"
                            >
                              <af:declarativeComponent
                                      viewId="/universal_taskflow/pages/edit_defaults/valueEditorWithElHtmlCompDef.jsff"
                                      id="dc_dc16">
                                  <f:attribute name="propertyClassName" value="universal_taskflow.common.data.SqlAttribute" />
                                  <f:attribute name="propertyName" value="filterAble" />
                                  <f:attribute name="objectId" value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.currentSqlAttribute.name}" />
                                  <f:attribute name="htmlDisabled" value="#{true}" />
                                  <f:attribute name="elDisabled" value="#{false}" />
                                  <f:facet name="content">
                                    <af:selectBooleanCheckbox id="dc_sbc8"
                                                              label="Фильтра"
                                                              simple="true"
                                                              value="#{backingBeanScope.editDefaultsBean.paramsForOnSql.currentSqlAttribute.filterAble}"
                                    />
                                  </f:facet>
                              </af:declarativeComponent>

                            </af:panelLabelAndMessage>

                            <af:panelLabelAndMessage id="dc_plam17"
                                                     label="Наличие cортировки в таблице"
                                                     labelStyle="text-align:start"
                                                     for="dc_dc17:dc_sbc9"
                                                     rendered="#{isAdfTable}"
                            >
                              <af:declarativeComponent
